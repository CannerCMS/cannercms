{
  "name": "@canner/bundle",
  "version": "0.0.3",
  "description": "Bundle the canner app and schema",
  "author": "Canner",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "web:statics:build": "MEASURE=true NODE_ENV=production ts-node src/bin/build.ts --output=../canner-server/.cms --schema=../canner-server",
    "web:statics:serve": "NODE_ENV=production ts-node src/bin/serve.ts",
    "web:dataSources": "NODE_ENV=production ts-node src/bin/dataSources.ts",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --timeout 10000 --compilers ts:ts-node/register --recursive --exit \"test/**/*.spec.ts\"",
    "clean": "rimraf lib dist",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.release.json --pretty && copyfiles -u 2 \"src/statics/**/*\" lib/statics",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.0.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@canner/antd-components": "^0.5.28",
    "@canner/container": "^2.0.37",
    "@canner/history-router": "^2.0.11",
    "@canner/storage": "2.0.11",
    "antd": "^3.11.5",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.9.1",
    "canner": "^2.0.45",
    "canner-schema-loader": "^2.0.37",
    "canner-script": "^2.0.36",
    "compression-webpack-plugin": "^2.0.0",
    "graphql": "^14.1.1",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.5",
    "mini-css-extract-plugin": "^0.5.0",
    "minimist": "^1.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-intl": "^2.8.0",
    "rimraf": "^2.5.4",
    "speed-measure-webpack-plugin": "^1.2.5",
    "style-loader": "^0.21.0",
    "styled-components": "^3.2.6",
    "tmp": "^0.0.33",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^4.3.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.0",
    "@types/lodash": "^4.14.107",
    "@types/minimist": "^1.2.0",
    "@types/node": "^9.4.6",
    "@types/webpack": "^4.4.22",
    "@types/webpack-dev-server": "^3.1.1",
    "copyfiles": "^2.1.0"
  },
  "peerDependencies": {
    "@canner/antd-components": "*",
    "@canner/container": "*",
    "@canner/history-router": "*",
    "@canner/storage": "*",
    "antd": "*",
    "canner": "*",
    "canner-schema-loader": "*",
    "canner-script": "*",
    "graphql": "*",
    "react": "*",
    "react-dom": "*",
    "react-intl": "*"
  },
  "repository": "Canner/canner",
  "license": "MIT"
}
