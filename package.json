{
  "name": "@canner/react-cms-core",
  "version": "0.0.5",
  "description": "the core-part of canner cms",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src test docs",
    "clean": "rimraf lib dist",
    "build:flow": "flow-copy-source -v -i '**/test/**' src lib",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files",
    "build:watch": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files --watch",
    "build": "npm run build:commonjs && npm run build:flow",
    "prepublish": "npm run clean && npm run check:src && npm run build",
    "check:src": "npm run lint",
    "start": "./node_modules/.bin/webpack-dev-server --mode development --config webpack.config.dev.js",
    "postpublish": "npm run build:docs",
    "build:docs": "./node_modules/.bin/webpack ./docs/index.js --output ./docs/static/bundle.js --mode production --config webpack.config.prod.js",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Canner/react-cms-core.git"
  },
  "keywords": [
    "canner",
    "cms",
    "core"
  ],
  "author": "Canner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Canner/react-cms-core/issues"
  },
  "homepage": "https://github.com/Canner/react-cms-core#readme",
  "devDependencies": {
    "@canner/canner-schema-loader": "^1.0.7",
    "@canner/canner-script": "^0.0.5",
    "@canner/cms-plugins-all": "^0.1.0-rc.35",
    "@canner/image-service-config": "^0.3.0",
    "@canner/react-cms-containers": "^0.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-emotion": "^9.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.18.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.3.0",
    "jest": "^23.0.0-alpha.0",
    "less": "2.7",
    "less-loader": "^4.0.6",
    "node-sass": "^4.7.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "@canner/cms-locales": "^0.1.0-rc.35",
    "@canner/react-cms-helpers": "^0.1.3",
    "antd": "^3.2.3",
    "bson-objectid": "^1.2.2",
    "canner-ref-id": "^0.1.2",
    "immutable": "4.0.0-rc.9",
    "invariant": "^2.2.3",
    "lodash": "^4.17.5",
    "promise-polyfill": "^7.1.0",
    "prop-types": "^15.6.1",
    "react-css-modules": "^4.7.1",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-spinners": "^0.2.6",
    "rxjs": "^5.5.6",
    "store": "^2.0.12",
    "styled-components": "^3.2.1"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=16.3"
  }
}
