{
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "lerna exec --ignore *graphql*  -- jest --coverage",
    "test:watch": "lerna --ignore *graphql*  exec -- jest --watch --coverage",
    "lint": "eslint ./packages/*/src/**/*.js",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna exec --parallel -- rimraf lib dist",
    "build:flow": "lerna exec --parallel --ignore *graphql* -- flow-copy-source -v -i '**/test/**' src lib",
    "build:watch": "gulp watch",
    "build:ts": "gulp ts",
    "build:js": "gulp js",
    "build": "gulp",
    "prepublishOnly": "npm run clean && npm run check:src && npm run build && npm run build:flow",
    "release": "npm run clean && npm run build && lerna publish --exact",
    "release:next": "npm run clean && npm run build && lerna publish --exact --npm-tag next",
    "check:src": "npm run lint",
    "start": "./node_modules/.bin/webpack-dev-server --mode development --config webpack.config.dev.js",
    "build:docs": "./node_modules/.bin/webpack ./docs/index.js --output ./docs/static/bundle.js --mode production --config webpack.config.prod.js",
    "validate": "npm ls"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.0.0",
